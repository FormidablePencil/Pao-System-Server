{
  "name": "paoserver",
  "version": "1.0.0",
  "description": "",
  "main": "dist/servers/mainServer.js",
  "scripts": {
    "start": "node -r ts-node/register",
    "main": "nodemon ./src/servers/mainServer.ts",
    "auth": "nodemon ./src/servers/authServer.ts",
    "startAuth:watch": "nodemon",
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "npm start",
    "ext": "ts"
  },
  "author": "Dennis Aleksandrov",
  "license": "ISC",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/mongoose": "^5.7.14",
    "@types/node": "^13.13.4",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.11",
    "nodemon": "^2.0.3",
    "passport": "^0.4.1",
    "passport-auth-token": "^1.0.1",
    "passport-google-oauth20": "^2.0.0",
    "ts-node": "^8.8.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/uuid": "^8.0.0",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  }
}
